<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyName>NetEti.$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>NetEti.DemoApplications</RootNamespace>
    <Title>$(AssemblyName) - Demoprogramm</Title>
    <Authors>Erik Nagel</Authors>
    <Company>NetEti</Company>
    <Description>$(AssemblyName) - Demopogramm</Description>
    <Copyright>Copyright © $(Authors) 2012-2023</Copyright>
    <AssemblyVersion>7.0.1.0</AssemblyVersion>
    <FileVersion>7.0.1.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="app.config.user" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\BasicAppSettings\BasicAppSettings.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="NetEti.AppEnvReader">
      <HintPath>..\..\..\ReadyBin\Assemblies\NetEti.AppEnvReader.dll</HintPath>
    </Reference>
    <Reference Include="NetEti.Global">
      <HintPath>..\..\..\ReadyBin\Assemblies\NetEti.Global.dll</HintPath>
    </Reference>
    <Reference Include="NetEti.InfoController">
      <HintPath>..\..\..\ReadyBin\Assemblies\NetEti.InfoController.dll</HintPath>
    </Reference>
    <Reference Include="NetEti.IniAccess">
      <HintPath>..\..\..\ReadyBin\Assemblies\NetEti.IniAccess.dll</HintPath>
    </Reference>
    <Reference Include="NetEti.Logging">
      <HintPath>..\..\..\ReadyBin\Assemblies\NetEti.Logging.dll</HintPath>
    </Reference>
    <Reference Include="NetEti.PropertyAccess">
      <HintPath>..\..\..\ReadyBin\Assemblies\NetEti.PropertyAccess.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>